.circle-main {
	position: relative;
	//margin: calc(75px + 2vw + 2vh) auto 0 auto;
	width: 88px;
	height: 88px;
	border-radius: 100px;
	overflow: hidden;
	cursor: pointer;
}
.circle-middle {
	position: absolute;
	top: 50%;
	left: 50%;
	width: 22px;
	height: 22px;
	margin-left: -11px;
	margin-top: -11px;
	border-radius: 22px;
	background: #ffffff;
	z-index: 50;
	will-change: transform;
}
.circle-outer {
	position: absolute;
	top: 50%;
	left: 50%;
	width: 44px;
	height: 44px;
	margin-top: -22px;
	margin-left: -22px;
	z-index: 49;
	background: none;
	border: 3px solid white;
	border-radius: 44px;
	transform: scale(0.78);
	-webkit-transform: scale(0.78);
	-moz-transform: scale(0.78);
	-o-transform: scale(0.78);
}
.circle-pulse {
	position: absolute;
	top: 50%;
	left: 50%;
	width: 44px;
	height: 44px;
	margin-top: -22px;
	margin-left: -22px;
	z-index: 48;
	background: none;
	border: 2px solid white;
	border-radius: 44px;
	opacity: 0;
	transform: scale(0.95);
	-webkit-transform: scale(0.95);
	-moz-transform: scale(0.95);
	-o-transform: scale(0.95);
	backface-visibility: hidden;
	-webkit-backface-visibility: hidden;
	animation-name: pulse;
	-webkit-animation-name: pulse;
	-moz-animation-name: pulse;
	-o-animation-name: pulse;
	animation-duration: 1.1s;
	-webkit-animation-duration: 1.1s;
	-moz-animation-duration: 1.1s;
	-o-animation-duration: 1.1s;
	animation-timing-function: cubic-bezier(0.165, 0.84, 0.44, 1);
	-webkit-animation-timing-function: cubic-bezier(0.165, 0.84, 0.44, 1);
	-moz-animation-timing-function: cubic-bezier(0.165, 0.84, 0.44, 1);
	-o-animation-timing-function: cubic-bezier(0.165, 0.84, 0.44, 1);
	animation-iteration-count: 1;
	-webkit-animation-iteration-count: 1;
	-moz-animation-iteration-count: 1;
	-o-animation-iteration-count: 1;
	will-change: opacity, transform;
}
.circle-middle,
.circle-outer {
	transition: transform 340ms cubic-bezier(0.405, -0.195, 0.165, 1.65),
		opacity 340ms ease-out;
	-webkit-transition: transform 340ms cubic-bezier(0.405, -0.195, 0.165, 1.65),
		opacity 340ms ease-out;
	-moz-transition: transform 340ms cubic-bezier(0.405, -0.195, 0.165, 1.65),
		opacity 340ms ease-out;
	-o-transition: transform 340ms cubic-bezier(0.405, -0.195, 0.165, 1.65),
		opacity 340ms ease-out;
}
.circle-main:hover .circle-outer {
	transform: scale(0.83);
	-webkit-transform: scale(0.83);
	-moz-transform: scale(0.83);
	-o-transform: scale(0.83);
	opacity: 1;
}
.circle-main:active .circle-outer {
	transform: scale(1.05);
	-webkit-transform: scale(1.05);
	-moz-transform: scale(1.05);
	-o-transform: scale(1.05);
}

